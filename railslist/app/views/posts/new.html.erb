<h1>create a new post</h1>
<ul>
  <%= form_for @post, url: {action: "create"} do |f| %>
    <li>
      <%= f.label :title %>
      <%= f.text_field :title %>
    </li>
    <li>
      <%= f.label :price %>
      <%= f.text_area :price %>
    </li>
    <li>
      <%= f.label :description %>
      <%= f.text_area :description %>
    </li>
    <li>
      <%= f.label :email %>
      <%= f.text_area :email %>
    </li>
    <li>
      <%= f.submit %>
    </li>
  <% end %>
</ul>

